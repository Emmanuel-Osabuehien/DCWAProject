<style>
  table,
  th,
  td {
    border: 1px solid black;
  }
</style>

<table>
  <h1>Countries</h1>
  <input type="submit" value="Add Country" onclick="location.href='/addCountry'">
  <br></br>
  <tr>
    <th>Country Code</th>
    <th>Country Name</th>
    <th>Country Details</th>
    <th>Action</th>

  </tr>
  <% countries.forEach((country) => { %>
  <tr>
    <td><%= country.co_code %></td>
    <td><%= country.co_name %></td>
    <td><%= country.co_details  %></td>
    <td><a href="/edit/<%= country.co_code %>">Update</a></td>
    <td><a href="/delete/<%= country.co_code %>">Delete</a></td>
  </tr>
  <% }) %>
</table>
<p><a href="/">Home</a></p>